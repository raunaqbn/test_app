<h1>Create a new testimonial </h1>
<%= form_for @testimonial do |f| %>
  <p>
      <%= f.label :title %> </br>
      <%=f.text_field :title %>
  </p>

  <p>
      <%= f.label :username %> </br>
      <%= f.text_field :username %>
  </p>
  
  <p>
      <%= f.label :email %></br>
      <%= f.text_field :email %>
  </p>
  <p>
      <%= f.label :description %></br>
      <%= f.text_area :description %>
  </p>
  <p>
      <%= f.submit %>
  </p>


<% end %>